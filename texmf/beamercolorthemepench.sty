\definecolor{penchblue}{RGB}{2,61,107}
\colorlet{color1}{penchblue}

\definecolor{penchlightblue}{RGB}{173,189,227}
\colorlet{color2}{penchlightblue}

\definecolor{penchgray}{RGB}{235,235,235}
\colorlet{penchgrey}{penchgray}
\colorlet{color3}{penchgray}

\definecolor{penchred}{RGB}{235, 95, 115}
\colorlet{color4}{penchred}

\definecolor{penchgreen}{RGB}{185, 210, 95}
\colorlet{color5}{penchgreen}

\definecolor{penchyellow}{RGB}{250, 235, 90}
\colorlet{color6}{penchyellow}

\definecolor{penchviolet}{RGB}{175, 130, 185}
\colorlet{color7}{penchviolet}

\definecolor{penchorange}{RGB}{250, 180, 90}
\colorlet{color8}{penchorange}

\definecolor{penchblack}{RGB}{0,0,0}
\definecolor{penchwhite}{RGB}{255,255,255}


\setbeamercolor{note page}{bg=white, fg=black}
\setbeamercolor{note title}{bg=penchlightblue!50!white, fg=black}
\setbeamercolor{note date}{fg=penchblue}

\setbeamercolor{structure}{fg=penchblue,bg=}
\setbeamercolor{palette primary}{use=structure,fg=penchblue,bg=}

\setbeamercolor{title}{fg=white,bg=penchblue}
\setbeamercolor{subtitle}{fg=penchlightblue}

\setbeamercolor{header}{fg=penchblue,bg=}
\setbeamercolor{navigation symbols}{fg=penchgray,bg=}

\setbeamercolor{frametitle}{fg=penchblue,bg=}
\setbeamercolor{framesubtitle}{fg=penchblue,bg=}

\setbeamercolor{footline}{fg=penchblue, bg=}

\setbeamercolor{inverse}{fg=penchwhite,bg=penchblue}

\setbeamercolor{normal text}{fg=black,bg=white}
\setbeamercolor{date in head/foot}{fg=structure}

\setbeamercolor{alerted text}{fg=penchred}
\setbeamercolor{example text}{fg=penchlightblue}
\setbeamercolor{emphasized text}{fg=penchblue}

\setbeamercolor{part name}{fg=white}
\setbeamercolor{part title}{fg=white}

\setbeamercolor{block title}{use=structure,fg=white,bg=structure.fg}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.fg}
\setbeamercolor{block title example}{use=example text,fg=white,bg=example text.fg}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=penchgray}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=penchgray}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=penchgray}


